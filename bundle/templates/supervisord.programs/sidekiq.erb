[program:<%= name %>]
command=bundle exec sidekiq
directory=<%= root %>
autostart=true  # env RUN_SIDEKIQ is active
autorestart=true
logfile_maxbytes=4MB
logfile_backups=10
stdout_logfile=<%= root %>/log/run.<%= name %>.log
stderr_logfile=<%= root %>/log/errors.<%= name %>.log
killasgroup=true
stopasgroup=true
user=decidim
